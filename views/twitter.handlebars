<section class="container">
  <h3 class="twitter-name">{{twitter.name}}</h3>
  <h4>Opening Hour: {{format_time twitter.starting_date}}</h4>
  <h4>Closing Hour: {{format_time twitter.ending_date}}</h4>
  <section class="flex-row justify-center">
    {{#each twitter.tweets as |tweets|}}
    {{> tweets-details}}
    {{/each}}
  </section>
</section>